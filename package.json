{
  "name": "aurora",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run build:aurora && npm run build:knex",
    "build": "next build",
    "build:aurora": "webpack --config tracker.config.js --mode production",
    "build:knex": "knex migrate:latest --env production",
    "db:seed": "knex seed:run",
    "db:test-seed": "npm run migrate:refresh && knex seed:run --specific=testing.js",
    "migrate:refresh": "knex migrate:rollback --all && knex migrate:latest",
    "start": "next start",
    "test": "npm run cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "axios": "^0.28.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.4",
    "cookie": "^0.4.1",
    "deepdash": "^5.3.5",
    "dotenv": "^8.2.0",
    "formik": "^2.2.6",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.0",
    "locale-codes": "^1.3.1",
    "md5": "^2.3.0",
    "next": "13.5.1",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-chartkick": "^0.4.1",
    "react-countup": "^4.3.3",
    "react-dom": "17.0.1",
    "react-table": "^7.6.3",
    "react-toast-notifications": "^2.4.3",
    "react-wavify": "^1.5.2",
    "swr": "^0.5.4",
    "ua-parser-js": "^0.7.33",
    "user-agents": "^1.1.122",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.12",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "cypress": "^13.6.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^2.0.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.6.0"
  }
}
